msgstr "Мрежа"

#: ../src/Engine/Session.cs:955
msgid "Protocol"
msgstr "Протокол"

#: ../src/Engine/Session.cs:956
msgid "Host"
msgstr "Домаћин"

#: ../src/Engine/Session.cs:957
msgid "Port"
msgstr "Прикључник"

