msgstr "Ağ"

#: ../src/Engine/Session.cs:955
msgid "Protocol"
msgstr "Protokol"

#: ../src/Engine/Session.cs:956
msgid "Host"
msgstr "Host"

#: ../src/Engine/Session.cs:957
msgid "Port"
msgstr "Port"

